{
  "name": "api",
  "version": "1.0.0",
  "description": "API REST server for Books",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rm -rf dist && webpack --mode development --config ./webpack.dev.config.js",
    "start": "node ./dist/index.js",
    "nodemon": "ls -la ./dist/ && nodemon --inspect ./dist/index.js",
    "watch:backEnd": "webpack --watch --mode \"development\" --config ./webpack.dev.config.js && nodemon --inspect ./dist/index.js",
    "start:backEnd": "npm-run-all -p nodemon watch:backEnd",
    "start:dev": "npm run build && npm run start:backEnd"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/preset-env": "^7.5.5",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "helmet": "^3.21.0",
    "moment": "^2.24.0",
    "mongoose": "^5.6.12",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-node-externals": "^1.7.2"
  }
}
